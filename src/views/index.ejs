                <section class="experience section">
                    <div class="section-inner">
                        <h2 class="heading">Work Experience</h2>
                        <div class="content">
                            <%for(var i = 0; i < person.jobs.length; i++){
                                var job = person.jobs[i];%>
                                <div class="item">
                                    <h3 class="title">
                                        <%=job.title%> - 
                                        <span class="place">
                                            <a href="<%=job.url%>" target="_blank"><%=job.company%></a>
                                        </span> 
                                        <span class="year">
                                            (<%=months[job.startDate.getMonth()] + " " + job.startDate.getFullYear()%> - <%=job.endDate ?  months[job.endDate.getMonth()] + " " + job.endDate.getFullYear(): 'Present' %>)
                                        </span>
                                    </h3>
                                    <p>
                                        <%=job.discription%>
                                    </p>
                                </div>
                                <%}%>
                        </div>
                    </div>
                </section>
                <section class="github section">
                    <div class="section-inner">
                        <h2 class="heading">My GitHub</h2>
                        <p>You can embed your GitHub contribution graph calendar using IonicaBizau's <a href="https://github.com/IonicaBizau/github-calendar" target="_blank">GitHub Calendar</a> widget.

                            <div id="github-graph" class="github-graph">

                            </div>
                            <!--//github-graph-->

                            <p>You can also embed your GitHub activities using Casey Scarborough's <a href="http://caseyscarborough.com/projects/github-activity/" target="_blank">GitHub Activity Stream</a> widget.
                                <!--//Usage: http://caseyscarborough.com/projects/github-activity/ -->
                                <div id="ghfeed" class="ghfeed">
                                </div>
                                <!--//ghfeed-->

                    </div>
                    <!--//secton-inner-->
                </section>
                <!--//section-->
            </div>
            <!--//primary-->
            <div class="secondary col-md-4 col-sm-12 col-xs-12">
                <aside class="info aside section">
                    <div class="section-inner">
                        <h2 class="heading sr-only">Basic Information</h2>
                        <div class="content">
                            <ul class="list-unstyled">
                                <li><i class="fa fa-map-marker"></i><span class="sr-only">Location:</span>
                                    <%=person.city%>,
                                        <%=person.state%> (
                                            <%=person.country%>)</li>
                                <li><i class="fa fa-envelope-o"></i><span class="sr-only">Email:</span>
                                    <a href="mailto:<%=person.email%>">
                                        <%=person.email%>
                                    </a>
                                </li>
                                <li><i class="fa fa-link"></i><span class="sr-only">Website:</span>
                                    <a href="<%=person.url%>">
                                        <%=person.displayUrl%>
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <!--//content-->
                    </div>
                    <!--//section-inner-->
                </aside>
                <!--//aside-->

                <aside class="skills aside section">
                    <div class="section-inner">
                        <h2 class="heading">Skills</h2>
                        <div class="content">
                            <p class="intro">
                                <%=person.skillsDescription%>
                            </p>

                            <div class="skillset">

                                <%for(var i = 0; i < person.skills.length; i++){
                                      var skill = person.skills[i];%>
                                    <div class="item">
                                        <h3 class="level-title">
                                        <%=skill.title%>
                                        <span class="level-label" data-toggle="tooltip" data-placement="left" data-animation="true" title="<%=skill.description%>">
                                            <%=skillLevel(skill.level)%>
                                        </span>
                                    </h3>
                                        <div class="level-bar">
                                            <div class="level-bar-inner" data-level="<%=skill.level%>%">
                                            </div>
                                        </div>
                                    </div>
                                    <%}%>

                                        <p><a class="more-link" href="https://coderwall.com/<%=person.coderwall%>"><i class="fa fa-external-link"></i> More on Coderwall</a></p>
                            </div>
                        </div>
                    </div>
                </aside>

                <aside class="education aside section">
                    <div class="section-inner">
                        <h2 class="heading">Education</h2>
                        <div class="content">
                            <%for(var i = 0; i < person.education.length; i++){
                                var edu = person.education[i]%>
                                <div class="item">
                                    <h3 class="title">
                                    <i class="fa fa-graduation-cap"></i> 
                                    <%=edu.degree%>
                                </h3>
                                    <h4 class="university">
                                    <%=edu.school%> 
                                    <span class="year">
                                        (<%=edu.startDate.getFullYear()%> - <%=edu.endDate ? edu.endDate.getFullYear() : "Present"%>)
                                    </span>
                                </h4>
                                </div>
                                <%}%>
                        </div>
                    </div>
                </aside>

